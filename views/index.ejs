<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width">
  <title><%- title %></title>
  <link rel="stylesheet" href="/css/style.css"></link>
  <script src="/components/d3/d3.min.js"></script>
  <% if (ENV_DEVELOPMENT) { %>
    <script src="http://localhost:35729/livereload.js"></script>
  <% } %>
</head>
<body>

  <% include navbar %>
  <div class="container">
    <div class="row">
      <div class="col s12 m10 l10" id="chart-container">
        <div class="card grey darken-3">
          <div class="card-content white-text">
            <div class="chart"></div>
          </div>
        </div>
      </div>
      <div class="col s12 m2 l2 chart-types">
        <div class="row">
          <div class="col s12 m12 l12">
            <div class="card grey darken-3">
              <div class="card-content white-text chart-type--selectors">
                <h6 class="white-text">Switch Type</h6>
                <a class="waves-effect waves-light btn" href="#" id="chart-type--pie">Pie</a>
                <a class="waves-effect waves-light btn disabled" href="#" id="chart-type--histogram">Histogram</a>
              </div>
            </div>
          </div>
          
          <div class="col s12 m12 l12">
            <div class="card grey darken-3">
              <div class="card-content white-text" id="attributes">
                <h6 class="white-text">Switch Attributes</h6>
              </div>
            </div>
          </div>

        </div>
      </div>
    </div>
  </div>

  <script src="js/index.js"></script>
  <script src="/components/jquery-csv/src/jquery.csv.min.js"></script>
  <script src="/components/materialize/dist/js/materialize.min.js"></script>

  </body>
</html>